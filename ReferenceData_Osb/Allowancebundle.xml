<!-- This xml contains the reference data for mapping between bundle category and allowance type -->
<allowanceBundles>
	<bundles>
		<bundle>
			<category>VoiceVolume</category>
			<bundleMapping>
				<firstAllowance>voiceAllowances</firstAllowance>
			</bundleMapping>
		</bundle>
	
