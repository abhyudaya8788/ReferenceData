			</bundleMapping>
		</bundle>
	
		<bundle>
			<category>MessageValue</category>
			<bundleMapping>
				<firstAllowance>valueAllowances</firstAllowance>
				<secondAllowance>messageValueAllowances</secondAllowance>
			</bundleMapping>
		</bundle>
	
		<bundle>
			<category>DataVolume</category>
			<bundleMapping>
				<firstAllowance>dataAllowances</firstAllowance>
			</bundleMapping>
		</bundle>
	
		<bundle>
			<category>DataUnlimited</category>
			<bundleMapping>
				<firstAllowance>unlimitedAllowances</firstAllowance>
